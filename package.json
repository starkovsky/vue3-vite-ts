{
  "name": "vue-next",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:force": "vite build",
    "serve": "vite preview",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache -q \"**/*.{vue,css}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "precommit": "yarn lint && yarn test && yarn build"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "js-cookie": "^3.0.5",
    "normalize.css": "8.0.1",
    "pinia": "^2.0.9",
    "qs": "6.11.2",
    "vue": "^3.2.26",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@types/jest": "29.5.3",
    "@types/js-cookie": "3.0.3",
    "@types/qs": "6.9.7",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/compiler-sfc": "3.3.4",
    "@vue/test-utils": "2.4.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-html": "7.1.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-vue": "9.16.1",
    "jest": "29.6.2",
    "jest-serializer-vue": "3.1.0",
    "jest-watch-typeahead": "2.2.2",
    "postcss": "8.4.27",
    "postcss-html": "1.5.0",
    "prettier": "3.0.1",
    "rollup-plugin-visualizer": "5.9.2",
    "stylelint": "15.10.2",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "tailwindcss": "3.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-plugin-html-env": "1.2.8",
    "vite-plugin-svg-icons": "2.0.1",
    "vue-eslint-parser": "9.3.1",
    "vue-tsc": "1.8.8",
    "vue3-jest": "27.0.0-alpha.2"
  }
}
