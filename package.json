{
  "name": "vue-next",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:force": "vite build",
    "serve": "vite preview",
    "lint:eslint": "yarn eslint --cache --max-warnings 0 --report-unused-disable-directives \"**/*.{js,ts,vue,html}\"",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:prettier": "yarn prettier --check \"**/*.{js,json,ts,vue,html}\"",
    "lint:prettier:fix": "yarn prettier --write \"**/*.{js,json,ts,vue,html}\"",
    "lint:stylelint": "yarn stylelint --cache -q \"**/*.{vue,css}\"",
    "lint:stylelint:fix": "yarn lint:stylelint --fix",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:stylelint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "precommit": "yarn lint && yarn test && yarn build"
  },
  "dependencies": {
    "axios": "0.24.0",
    "js-cookie": "3.0.1",
    "normalize.css": "8.0.1",
    "qs": "6.10.1",
    "vue": "3.2.19",
    "vue-router": "4.0.12",
    "vuex": "4.0.2"
  },
  "devDependencies": {
    "@types/jest": "27.0.2",
    "@types/js-cookie": "3.0.0",
    "@types/qs": "6.9.7",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "@vitejs/plugin-vue": "1.9.4",
    "@vue/compiler-sfc": "3.2.20",
    "@vue/test-utils": "2.0.0-rc.16",
    "autoprefixer": "10.4.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-vue": "7.20.0",
    "jest": "27.3.1",
    "jest-serializer-vue": "2.0.2",
    "jest-watch-typeahead": "1.0.0",
    "postcss": "8.3.11",
    "postcss-html": "1.2.0",
    "prettier": "2.4.1",
    "rollup-plugin-visualizer": "5.5.2",
    "stylelint": "14.0.1",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended-vue": "1.0.0",
    "stylelint-config-standard": "23.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "3.0.5",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4",
    "vite": "2.6.13",
    "vite-plugin-html-env": "1.0.3",
    "vite-plugin-svg-icons": "1.0.5",
    "vue-eslint-parser": "8.0.0",
    "vue-tsc": "0.28.10",
    "vue3-jest": "27.0.0-alpha.2"
  }
}
